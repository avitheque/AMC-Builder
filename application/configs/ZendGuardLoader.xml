<?xml version="1.0" encoding="UTF-8"?>
<guardProject xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" appName="AMCbuilder" appVersion="1" targetDir="E:\www\release\amc-builder" xsi:noNamespaceSchemaLocation="guard.xsd">
	
	<options>
		<phpVersion value="5.4"/>

		<phpFilesExtensions>
			<extension>php</extension>
			<extension>inc</extension>
			<extension>php3</extension>
			<extension>php4</extension>
			<extension>phtml</extension>
		</phpFilesExtensions>

		<supportShortTags value="true"/>
		<supportASPTags value="false"/>
		<workWithEncodedFilesOnly value="false"/>
		
		<expiration>
			<none/>
		</expiration>

		<licenseType value="none"/>
		<privateKey>
			<keyData/>
		</privateKey>

		<headerInformation>
			<default/>
		</headerInformation>

		<optimizationLevel value="full"/> 
		<ignoreErrors value="true"/>
		<verbosityLevel value="verbose"/>
		<allowEncodedFileModification value="true"/>
		<resolveSymlinks value="true"/>
		
		<securityMethodList>
